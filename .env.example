# Mac Contacts Export to Google Sheets - Environment Variables
# Copy this file to .env and fill in your values

# Google Sheets Configuration
# The ID from your Google Sheet URL
# Example: If your sheet URL is https://docs.google.com/spreadsheets/d/1ABC123xyz/edit
# Then your SHEET_ID = 1ABC123xyz
GOOGLE_SHEET_ID=your_sheet_id_here

# Optional: Sheet name (defaults to "Contacts")
SHEET_NAME=Contacts

# Optional: Export format (json or csv)
# If you want local backups in addition to Google Sheets
EXPORT_FORMAT=json

# Optional: Export directory for local backups
# Defaults to ./exports
EXPORT_DIR=./exports

# Optional: Verbose output
# Set to True to see detailed progress
VERBOSE=True